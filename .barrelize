{
  "$schema": "node_modules/barrelize/schema.json",
  "semi": false,
  "singleQuote": true,
  "barrels": [
    {
      "name": "index.ts",
      "root": "types",
      "exclude": [
        "**/*.test.ts",
        "**/*.d.ts"
      ]
    },
    {
      "name": "index.ts",
      "root": "src",
      "include": [
        "**/attribute.ts"
      ],
      "exports": {
        "**/attribute.ts": [
          "/^(?!default$)(.+)/ as $1"
        ],
        "**/Arr.ts": [
          "Arr",
          "/^(?!default$)(.+)/ as $1"
        ],
        "**/Crypto.ts": [
          "* as Crypto",
          "/^(?!default$)(.+)/ as $1"
        ]
      }
    }
  ]
}